            </StackPanel>
            <StackPanel Orientation="Horizontal" Visibility="Collapsed" Margin="0" Padding="0" >
                <Button x:Name="btnStartTalk" Click="btnStartTalk_Click" IsEnabled="{Binding IsReadyToRecognize}" Margin="0,0,0,0" >
                    <StackPanel Orientation="Horizontal">
                        <SymbolIcon Symbol="Microphone"/>
                        <TextBlock x:Name="StartTalkButtonText" Text="Start Talk" />
                    </StackPanel>
                </Button>
                <Button x:Name="btnClearText" Click="btnClearText_Click"  IsEnabled="False" Margin="10,0,0,0" Content="Clear Text" />
            </StackPanel>
            <TextBox x:Name="dictationTextBox" Margin="-130,50,0,0" IsReadOnly="True" VerticalAlignment="Bottom" TextWrapping="Wrap" ScrollViewer.VerticalScrollBarVisibility="Auto" Height="60" Width="300" TextChanged="dictationTextBox_TextChanged"/>
            <StackPanel Orientation="Horizontal"  Margin="0,5,0,0">
                <TextBlock x:Name="checkError" Text="Speech Recog Status:" Margin="0,10,10,5" Visibility="Collapsed"></TextBlock>
                <TextBox x:Name="errorCheck" FontSize="16" Height="Auto" Margin="0,0,0,0" Visibility="Collapsed" Width="440"/>
            </StackPanel>
        </StackPanel>

        <StackPanel Orientation="Vertical" Margin="0,300,0,10"  Visibility="Collapsed">

            <StackPanel Orientation="Vertical" Margin="0,20,0,0" >
                <TextBlock Text="Original msg Received:" FontSize="16" Height="20"  Margin="0,10,150,0" />
                <TextBox x:Name="originalmsg" Margin="0,0,0,0" IsReadOnly="True" TextWrapping="Wrap" ScrollViewer.VerticalScrollBarVisibility="Auto" />
                <TextBlock Text="Translated msg:" FontSize="16" Height="20"  Margin="0,10,150,0" />
                <TextBox x:Name="ReceivedText" Margin="0,0,0,10" IsReadOnly="True" TextWrapping="Wrap" ScrollViewer.VerticalScrollBarVisibility="Auto" />
            </StackPanel>
            <StackPanel Orientation="Horizontal">
                <TextBlock Text="Status Check:" FontSize="16" Height="20" Margin="0,5,10,0" />
                <TextBox x:Name="StatusText" Text="Idle" FontSize="16" Height="Auto" Margin="0,5,10,0" Width="474" />
            </StackPanel>

        </StackPanel>
        <MediaElement x:Name="media" AutoPlay="True" Margin="180,355,161.667,354" MediaEnded="media_MediaEnded" MediaFailed="media_MediaFailed" />

    </Grid>
</Page>
